-- Gui to Lua
-- Version: 3.2

-- Instances:

local nolagbag = Instance.new("ScreenGui")

--Properties:

nolagbag.Name = "no lag bag"
nolagbag.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
nolagbag.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

-- Scripts:

local function LLYORVN_fake_script() -- nolagbag.The No Lag Bag 
	local script = Instance.new('LocalScript', nolagbag)

	require(game:GetService("ReplicatedStorage").Game.Notification).new(
	{Text = "Thanks You For Using UnPacked's Anti Lag Script :D, The game may look bad but no lag so that's not bad eh?", Duration = 5
	})
end
coroutine.wrap(LLYORVN_fake_script)()
local function CTHCM_fake_script() -- nil.No Lag 
	local script = Instance.new('LocalScript', nil)

	local a = game
	local b = a.Workspace
	local c = a.Lighting
	local d = b.Terrain
	d.WaterWaveSize = 0
	d.WaterWaveSpeed = 0
	d.WaterReflectance = 0
	d.WaterTransparency = 0
	c.GlobalShadows = false
	c.FogEnd = 9e9
	settings().Rendering.QualityLevel = "Level01"
	for e, f in pairs(a:GetDescendants()) do
		if f:IsA("Part") or f:IsA("Union") or f:IsA("CornerWedgePart") or f:IsA("TrussPart") then
			f.Reflectance = 0
		elseif f:IsA("Decal") or f:IsA("Texture") then
			f.Transparency = 0
		elseif f:IsA("ParticleEmitter") or f:IsA("Trail") then
			f.Lifetime = NumberRange.new(0)
		elseif f:IsA("Explosion") then
			f.BlastPressure = 0
			f.BlastRadius = 0
		elseif f:IsA("Fire") or f:IsA("SpotLight") or f:IsA("Smoke") or f:IsA("Sparkles") then
			f.Enabled = false
		elseif f:IsA("MeshPart") then
			f.Reflectance = 0
		end
		if f:IsA("Fire") then
			f:Destroy()
			wait()
		end
		if f:IsA("ParticleEmitter") then
			f:Destroy()
			wait()
		end
	end
	for e, g in pairs(c:GetChildren()) do
		if
			g:IsA("BlurEffect") or g:IsA("SunRaysEffect") or g:IsA("ColorCorrectionEffect") or g:IsA("BloomEffect") or
				g:IsA("DepthOfFieldEffect")
		then
			g.Enabled = false
		end
	end
	sethiddenproperty(game.Lighting, "Technology", "Compatibility")
end
coroutine.wrap(CTHCM_fake_script)()
local function TKISK_fake_script() -- nil.UnPacked Script Sayer 2 
	local script = Instance.new('LocalScript', nil)

	require(game:GetService("ReplicatedStorage").Game.Notification).new(
	{Text = "Join Our Discord For Daily Updates And More Scripts :D.Discord.io/UnPacked.", Duration = 5
	})
end
coroutine.wrap(TKISK_fake_script)()
